   "source": [
    "x=int(input(\"enter the number :\"))\n",
    "\n",
    "is_prime=True\n",
    "check_num=x\n",
    "for i in range(2,check_num):\n",
    "    if x % i == 0:\n",
    "        is_prime=False\n",
    "if is_prime:\n",
    "        print(f\"{x} is a prime number\")\n",
    "else:\n",
    "    print(f\"{x} is not a prime number\")"
